{
  "presets": [ "es2015", "stage-0" ],
  "plugins": [ "transform-runtime", "add-module-exports" ],
  "env": {
    "development": {
      "plugins": [
        [ "react-transform", {
          "transforms": [ {
            "transform": "react-transform-hmr",
            "imports": [ "react" ],
            "locals": [ "module" ]
          } ]
        } ]
      ]
    }
  }
}
